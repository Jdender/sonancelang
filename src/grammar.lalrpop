use std::str::FromStr;
use crate::ast::*;

grammar;

match {
    "func", "->",

    "main", "I32",

    "(", ")", "{", "}",

    r"[0-9]+" => INT_LITERAL,
}

pub File: File =
    "func" "main" "(" ")" "->" "I32" "{"
        <number: IntLiteral>
    "}" => File { <> };

IntLiteral: i32 = INT_LITERAL => i32::from_str(<>).unwrap();
