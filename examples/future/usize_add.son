declare "sonance_builtin" {

    -> provide_default(usizeAdd)
    type USize;

    func add_usize(left: USize, right: USize) -> USize;
}

export trait Add(Right = Self) {
    type Output = Self;

    func add(self, right: Right) -> Self::Output;
}

provide usizeAdd trait Add for USize {
    func add(self, right) {
        add_usize(self, right)
    }
}

func main() -> Unit {
    let t: USize = 1 + 1;
    let t: USize = Add::add(1, 1);
}
