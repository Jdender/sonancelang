func main() -> Unit {
    for i in 0..=100 {
        print(fizzbuzz(i));
    }
}

func fizzbuzz(num: U32) -> StringBuf {
    let pair = struct { num % 3, num % 5 };
    match pair {
        struct { 0, 0 } -> "Fizzbuzz",
        struct { 0, _ } -> "Fizz",
        struct { _, 0 } -> "Buzz",
        _ -> num.to_string(),
    }
}
